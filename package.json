{
  "name": "veggie",
  "version": "3.0.0-beta.10",
  "description": "Interactive mock server for profiling user scenarios",
  "main": "dist/veggie.js",
  "module": "dist/veggie.es.js",
  "browser": "dist/veggie.api.js",
  "bin": {
    "veg": "bin/veg"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:test": "rollup -c rollup.test.config.js",
    "commit": "commit",
    "coverage": "nyc report --reporter=lcov | coveralls",
    "preversion": "npm test",
    "version": "npm run build # Get latest version for banners",
    "postversion": "git push && git push --tags && npm publish",
    "preserve": "npm run build",
    "serve": "bin/veg --dir test/services/*.js --profileDir test/profiles",
    "serve:watch": "nodemon -w src -x 'npm run serve'",
    "pretest": "standard && npm run build:test",
    "test": "nyc --reporter=html --reporter=text mocha dist/veggie.test.js",
    "test:watch": "nodemon -w src --ignore test/profiles -w test -x 'npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micburks/veggie.git"
  },
  "keywords": [
    "mock",
    "server",
    "interactive",
    "express",
    "services",
    "repl"
  ],
  "author": "Mickey Burks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micburks/veggie/issues"
  },
  "homepage": "https://github.com/micburks/veggie#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "get-port": "^3.2.0",
    "glob": "^7.1.2",
    "meow": "^3.7.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^5.2.3",
    "@commitlint/prompt-cli": "^5.2.8",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "coveralls": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "parallelshell": "^3.0.2",
    "rollup": "^0.55.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-istanbul": "^2.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "standard": "^10.0.3"
  },
  "files": [
    "bin",
    "dist/veggie.js",
    "dist/veggie.api.js",
    "dist/veggie.es.js"
  ]
}
